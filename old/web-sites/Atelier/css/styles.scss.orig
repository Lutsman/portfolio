  height: 630px;
  h2 {
    line-height: 128px;
  }
  .top {
    width: 314px;
    height: 320px;
    top: 175px;
    right: 93px;
    border-color: rgba(225, 208, 196, 0.9);
  }
  .bottom {
    width: 314px;
    height: 320px;
    top: 195px;
    right: 73px;
    border-color: $title-brown;
  }
}
.info {
  width: 324px;
  height: 330px;
  padding: 25px 0 0 40px;
  top: 185px;
  right: 83px;
  position: absolute;
  background-color: rgba(255, 255, 255, 0.949);
  @include box-sizing(border-box);
  z-index: 2;
  h3 {
    padding-bottom: 10px;
    margin-top: 0;
    color: $title-brown;
  }
  p {
    margin-top: 15px;
    font: 18px $cuprum-reg;
  }
  span {
    padding-top: 7px;
    display: block;
    line-height: 18px;
  }
  b {
    padding-top: 2px;
    padding-bottom: 1px;
    display: block;
    font-size: 24px;
    color: $text-grey;
    line-height: 24px;
  }
  i {
    line-height: 32px;
  }
}
.info-text {
  background-color: transparent;
  z-index: 5;
}
.map {
  width: 100%;
  height: 525px;
  position: absolute;
  top: 105px;
  z-index: 1;
}

/* ----------------------------------------------
Pop up
---------------------------------------------- */

.call-b {
  margin-left: -222px;
  margin-top: -152.5px;
  position: fixed;
  display: none;
  top: 50%;
  left: 50%;
  opacity: 1;
  z-index: 100;
  p:last-child {
    font-size: 10px;
    color: $header-grey;
    letter-spacing: -0.3px;
  }
}
.ask-q {
  width: 578px;
  height: 341px;
  margin-left: -289px;
  margin-top: -170.5px;
  display: none;
  position: fixed;
  top: 50%;
  left: 50%;
  z-index: 100;
  p:last-child {
    font-size: 10px;
    color: $header-grey;
    letter-spacing: -0.3px;
  }
  form {
    width: 498px;
    text-align: center;
    div {
      float: left;
      text-align: left;
    }
    div + div {
      margin-left: 20px;
    }
  }
  input {
    width: 239px;
  }
  textarea {
    width: 239px;
    height: 164px;
    padding-top: 10px;
    padding-left: 15px;
    margin-bottom: 7px;
    color: #A09F9F;
    background-color: #FFF;
    border: 2px solid $border-input;
    @include border-radius(5px);
    @include box-sizing(border-box);
  }
  input[type="submit"] {
    width: 356px;
  }
  .form-title {
    background-position: 107px -126px;
  }
}
.close {
  position: absolute;
  top: -24px;
  right: -25px;
  a{
    width: 26px;
    height: 26px;
    display: block;
    font-size: 20px;
    font-weight: bold;
    color: #4b4b4b;
    line-height: 26px;
    background-color: #e09d9e;
    @include  border-radius(50%);
    &:hover {
      background-color: $btn-hover-bg;
    }
  }
}
.form-success {
  border-color: green !important;
}
.form-fail {
  border-color: red !important;
}
.pop-up-wrap {
  top:0;
  position: fixed;
  width:100%;
  min-height:100%;
  height: 100%;
  background-color: rgba(0,0,0,0.5);
  overflow:hidden;
  display: none;
  z-index: 99;
}